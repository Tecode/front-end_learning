{
  "name": "yeoman-node",
  "version": "1.0.0",
  "description": "Yeoman Study",
  "homepage": "",
  "author": "haoxuan",
  "files": [
    "lib"
  ],
  "main": "index.js",
  "keywords": [
    "Yeoman Front-End"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^26.1.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "generator-node": "^2.8.0",
    "yo": "^4.3.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "git@github.com:Tecode/front-end_learning.git",
  "jest": {
    "testEnvironment": "node"
  }
}
